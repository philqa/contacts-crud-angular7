{
  "name": "contacts-crud-angular7",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "start:server": "npm start",
    "start:mock": "forever start mockServer.js",
    "start:both": "start-test start:mock http-get://localhost:4000/ start:server",
    "e2e:ci": "start-test start:both http-get://localhost:4200/ codecept",
    "codecept": "codeceptjs run -c ./e2e --steps --verbose --reporter mocha-multi",
    "visual:base": "codeceptjs run -c ./e2e --steps --verbose --reporter mocha-multi --profile base",
    "parallel": "codeceptjs run-multiple parallel -c ./e2e --steps --verbose --reporter mocha-multi"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "bootstrap": "^4.1.3",
    "core-js": "^2.5.4",
    "ng2-slim-loading-bar": "^4.0.0",
    "rxjs": "~6.3.3",
    "rxjs-compat": "^6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codeceptjs": "^2.0.8",
    "codelyzer": "~4.5.0",
    "forever": "^0.15.3",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "mocha-junit-reporter": "^1.18.0",
    "mocha-multi": "^1.0.1",
    "mochawesome": "^3.1.1",
    "ng-apimock": "^1.4.8",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.4.0",
    "protractor": "^5.4.2",
    "puppeteer": "^1.11.0",
    "start-server-and-test": "^1.7.12",
    "ts-node": "~5.0.1",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
